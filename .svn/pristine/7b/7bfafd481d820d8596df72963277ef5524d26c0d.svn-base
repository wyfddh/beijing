@import "reset";
$fontSize:64;
@function px2em($px) {
  @return $px/$fontSize + rem;
}
@mixin font-dpr($font-size){
  font-size: $font-size;

  [data-dpr="2"] & {
    font-size: $font-size * 2;
  }

  [data-dpr="3"] & {
    font-size: $font-size * 3;
  }
}

    <if test="orgId == null and orgList != null and currentOrg != null">
    	)
    </if>
    order by if(a.publish='3',0,1), a.createTime desc
    <if test="pageSize != 0">
    	<if test=" startRow == 0">
			limit #{pageSize}
    	</if>
    	<if test=" startRow != 0">
			limit #{startRow},#{pageSize}
    	</if>
	</if>
  </select>
  <select id="countListByKey" resultType="java.lang.Integer" >
    select count(1)
      from mip_wenchuang a
			left JOIN mip_organization b on a.org_id=b.id
			left join mip_wenchuang_category c on a.category_id=c.id
			left join mip_user d on(d.`status`=1 and d.id=a.user_id)
    where 1=1
    <if test="key != null and key != ''">
    	and (a.product_number like concat('%', #{key}, '%')
    	   or a.product_name like concat('%', #{key}, '%'))
    </if>
    <if test="publish != null and publish != ''">
    	and a.publish = #{publish}
    </if>
    <if test="categoryId != null and categoryId != ''">
    	and a.category_id = #{categoryId}
    </if>
    <if test="staPrice != null and staPrice != ''">
    	<![CDATA[ and a.product_price >= #{staPrice}   ]]> 
    </if>
    <if test="overPrice != null and overPrice != ''">
    	<![CDATA[ and a.product_price <= #{overPrice}   ]]> 
    </if>
    <if test="orgId != null and orgId != ''">
    	and a.org_id = #{orgId}
    </if>
    <if test="orgId == null and orgList != null and currentOrg != null">
    	and ( a.org_id = #{currentOrg}
    </if>
    <if test="orgId == null or orgId == ''">
    	<if test="orgList != null">
	    	or (a.org_id in
	    	<foreach collection="orgList" item="item" open="(" close=")" separator="," >
	           #{item}
	        </foreach>
	        and a.publish in('1', '3'))
        </if>
    </if>
    <if test="orgId == null and orgList != null and currentOrg != null">
    	)
    </if>
  </select>
  <select id="getReleList" resultType="com.tj720.mip.model.MipWenchuang" >
    select a.id, a.category_id as categoryId, a.product_price as productPrice, a.user_id as userId, a.picture_id as pictureId, a.publish_time as publishTime, 
	      a.publish, a.product_number as productNumber, a.product_name as productName, a.design_elements as designElements, a.org_id as orgId, a.status, 
	      a.sequence, a.createTime, a.type, a.reason, a.content ,
	      b.`name` as musExhibition, c.category_name as categoryName , d.`name` as nickName
      from mip_wenchuang a
			left JOIN mip_organization b on a.org_id=b.id
			left join mip_wenchuang_category c on a.category_id=c.id
			left join mip_user d on(d.`status`=1 and d.id=a.user_id)
    where 1=1
    	and a.publish = '1'
    	and a.category_id = #{categoryId}
    	and a.id not in(#{currentId})
    	limit #{pageSize}
  </select>
</mapper>
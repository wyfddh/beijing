.checkList{
	padding: 30px;
	display: none;
}
.checkList>div{
	margin: 12px 0;
}
.checkList input{
	line-height: 26px;
    border-radius: 5px;
}
.checkList select{
	line-height: 26px;
    border-radius: 5px;    
    width: 190px;
    padding: 4px 4px 4px 20px;
    border: solid 1px #ddd;
}
.checkList .star{
	margin-top: 20px;
}
.checkList .star button{
	background: #2A9BCF;
    color: #fff;
    border-radius: 5px;
}
.checkList .star button.b2{
	background: #fff;
    color: #2A9BCF;
    border-color: #2A9BCF;
}



.checkList_table{
	
	background-color: #F1F2F7;
	height: 66px;
	padding-top: 20px;
	box-sizing: border-box;
	
}
.checkList_table>a{
	display: block;
	text-decoration: none;
	padding: 16px 38px 16px;
	border-top-left-radius:5px ;
	border-top-right-radius: 5px;
	color: black;
	float: left;
	font-size: 14px;
  	font-family: "Microsoft YaHei";
  	color: black;
  	line-height: 14px;
  	height: 14px;
  	box-sizing: content-box;
  	cursor: pointer;
}

.checkList_table_active{
	color: rgb(42, 155, 207) !important;
	background-color: white;
}

/*弹窗样式*/
.checkList_title{
	border: 1px solid #f1f2f7;
	width: 214px;
	padding: 6px 10px;
	font-size: 14px;
	background-color: #fcfcfc;
	border-radius: 5px;
}
.checkList_content{
	border: 1px solid #f1f2f7;
	width: 620px;
	height: 296px;
	padding: 6px 10px;
	font-size: 14px;
	background-color: #fcfcfc;
	border-radius: 5px;
	resize:none;
}
.zj_checkList_save{
    display: block;
    float: left;
	background-color: #2a9bcf;
	color: white;
	padding: 9px 15px 9px 35px;
	border-radius: 5px;
	text-decoration: none;
	height: 18px;
	line-height: 18px;
	border: 1px solid #2a9bcf;
	height: 36px;
	background-image: url(../images/save.png);
	background-repeat: no-repeat;
	background-position: 10px center;
	margin: 32px 0 15px 0px;
	cursor: pointer;
}
.zj_checkList_cancel{
	display: block;
	float: left;
	background-color: white;
	color: #2a9bcf;
	padding: 9px 15px 9px 35px;
	border-radius: 5px;
	text-decoration: none;
	height: 18px;
	line-height: 18px;
	border: 1px solid #2a9bcf;
	height: 36px;
	background-image: url(../images/cancel.png);
	background-repeat: no-repeat;
	background-position: 10px center;
	margin: 32px 0 15px 15px;
	cursor: pointer;
}


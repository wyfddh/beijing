		<if test="name != null and name != ''">
			and a.buss_name like '%${name}%'
		</if>
		GROUP BY a.id
		order by max(b.create_time) desc
  </if>
  </select>
  
  <select id="selectReportById" resultType="java.util.Map">
	  select a.id, a.define_id, a.buss_name, b.define_code, a.buss_code
	  from cgform_buss a
	  left join cgform_define b on (a.define_id=b.id)
	  where a.id=#{id}
  </select>
  
  <select id="selectListByOrgId" resultType="com.tj720.admin.model.GovNotice">
	  select a.id, a.title, a.content, a.publisher, a.publishOrg, a.create_time as createTime,
	  a.lastUpdate_time as lastupdateTime, a.isfeedback, a.publish_status as publishStatus, a.attachment_ids as attachmentIds,
	  a.deadline_time as deadlineTime, a.isFill as isfill , a.creater, a.reportCode, 
	  count(b.id) as totalWriteNum,count(b.id) as totalLookNum, sum(if(b.receive_status in('1', '2', '4', '5'),1,0)) as isLookNum,
	 sum(if(b.receive_status = '4',1,0)) as isWriteNum
	  from gov_notice a
	  left join gov_notice_org b on(a.id=b.notice_id)
	  left join mip_organization c on(b.org_id=c.id)
		where a.publish_status = 2
		and c.status = '1'
		and a.publishOrg = #{orgId}
		group by a.id
		ORDER BY a.create_time desc
		limit 4
  </select>
  
  <select id="getList" resultType="com.tj720.admin.model.GovNotice">
	select a.id, a.title, a.content, a.publisher, a.publishOrg, a.create_time as createTime,
	  	a.lastUpdate_time as lastupdateTime, a.isfeedback, a.publish_status as publishStatus ,
		b.`name` as publisherName, c.`name` as publishOrgName,a.deadline_time as deadlineTime, 
		a.isFill as isfill , a.creater, a.reportCode
	from gov_notice a
	left join mip_user b on(a.publisher = b.id)
	left join mip_organization c on(a.publishOrg = c.id)
	where 1=1
	<![CDATA[ and a.publish_status <> -1 ]]> 
	<if test="data.title != null and data.title != ''">
		and a.title like concat('%', #{data.title} , '%')
	</if>
	<if test="data.startDate != null and data.startDate != '' and data.endDate != null and data.endDate != ''">
		and a.publish_status = '2'
		and a.lastUpdate_time BETWEEN #{data.startDate} and #{data.endDate}
	</if>
	<if test="data.publishStatus != null and data.publishStatus != ''">
		and a.publish_status = #{data.publishStatus}
	</if>
	<if test="data.publisher != null and data.publisher != ''">
		and a.publisher = #{data.publisher}
	</if>
	<if test="data.publishorg != null and data.publishorg.size() > 0">
		and a.publishOrg in
		<foreach collection="data.publishorg" item="item" open="(" close=")" separator="," >
		   #{item}
		</foreach>
	</if>
	ORDER BY a.create_time desc
	<if test="data.startRow == 0">
	limit #{data.pageSize}
	</if>
	<if test="data.startRow != 0">
	limit #{data.startRow}, #{data.pageSize}
	</if>
  </select>
  
  <select id="countList" resultType="java.lang.Integer">
	select count(1)
	from gov_notice a
	where 1=1
	<![CDATA[ and a.publish_status <> -1 ]]> 
	<if test="data.title != null and data.title != ''">
		and a.title like concat('%', #{data.title} , '%')
	</if>
	<if test="data.startDate != null and data.startDate != '' and data.endDate != null and data.endDate != ''">
		and a.publish_status = '2'
		and a.lastUpdate_time BETWEEN #{data.startDate} and #{data.endDate}
	</if>
	<if test="data.publishStatus != null and data.publishStatus != ''">
		and a.publish_status = #{data.publishStatus}
	</if>
	<if test="data.publisher != null and data.publisher != ''">
		and a.publisher = #{data.publisher}
	</if>
	<if test="data.publishorg != null and data.publishorg.size() > 0">
		and a.publishOrg in
		<foreach collection="data.publishorg" item="item" open="(" close=")" separator="," >
		   #{item}
		</foreach>
	</if>
  </select>
  
  
</mapper>
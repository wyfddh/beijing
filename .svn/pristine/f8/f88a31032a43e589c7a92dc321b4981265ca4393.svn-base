@charset "utf-8";
body,ul,li{
	margin: 0;
	padding: 0;
}

.page-content{
	width: 100%;
	max-width: 1920px;
	min-width:530px;
}
.ovf-aut{
	overflow: auto;
}
.tab table{
	overflow-y: auto;	
}
.tab th{
	overflow: hidden;
	word-wrap: break-word;	
	width: 150px;
	/*height: 20px;
	line-height: 20px;
	text-align:right;
	*/
	
	padding-right: 10px;
}

.tab input{
	width:95%;
	border: none;		
}
input[type="text"]{
	margin-bottom: 0;
	/*height: 20px;*/
	border-bottom: 1px solid  rgba(239, 243, 248,1);
	
}
.rad{
	color: #FF0000;
}
.plus-con{
	float: right;
	overflow-x: auto;
}

.plus-nav{
	height:1000px;
	float: left;
	margin-top: 10px;
	border: 1px solid #CCCCCC ;
	background-color:#F7F7F7;
  
}
.text-top{
	padding: 15px;
	
	background-color:#abbac3  ;
}
.text-top>h4{
	display: inline-block;
}
.text-top>a{
	float: right;
	margin:15px 5px 0 0;
	color: #FFFFFF;
}
#window-al,#window-a2{
	width:700px;
	height: 500px;
	background-color:#ddd;
	border: 1px solid #CCCCCC;
	position: absolute;
	left: 250px;
	top:70px ;
	color: #000;
	z-index: 999;
}
#window-a3{
			width: 80%;	
			min-width: 500px;
		   	position: absolute;
		   	top: 70px;
		   	left: 30px;
	        z-index: 999;
		}
#window-a2{
	position: absolute;
	left:200px;
	top:100px ;
}

.header{
	padding: 5px;
	border: none;	
	margin: 0;
	color: #EEEEEE;
	background-color: #428bca;
	
}
.del{
	float: right;
	color:#FFFFFF;
	margin-right: 10px;
}

.del:hover{
	color: #FF0000;
}

.up-cen a:hover{
	color: #FF0000;
}
.upload{
	padding: 5px;
	line-height:20px;
	border-bottom:1px solid #ccc;
}
.upload,.from-up>.bg{
	background-color:#eee;
	opacity: 0.6;
	height:30px;
}
.bg{
	border-bottom:1px solid #eee;
}
.from-up{
	text-align: center;
}
.up-cen{
	width: 695px;
	height:200px;
	background-color: #FFFFFF;
	margin: 2px auto;
}

.progress{
	margin-bottom: 0;
}
.act{
	height: 90px;
	border: 1px solid #CCCCCC;
	overflow: auto;
	background-color: #FFFFFF;
}
.act a:hover{
 color: #009CEB;
}
.act a>i:hover{
	color: #FF0000;
}
.act a>i{
	color: #ccc;
}
.act p{
	width: 200px;
	float: left;
	background-color:#E1EDF7;
	margin:5px 0 0 10px;
}

.up-cen a:hover{
	color: #FF0000;
}


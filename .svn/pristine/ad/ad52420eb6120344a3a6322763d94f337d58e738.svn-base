    mveh.viName as viname, mveh.viClassify as viclassify,mveh.viPCUrl as vipcurl,mveh.viMoveUrl as vimoveurl,mveh.viIntro as viintro,
    mveh.status as status, mveh.org_id as orgId,mveh.picture_id as pictureId,mveh.user_name as userName,mveh.createTime as createtime,mo.name as orgName
    from mip_virtual_exibition_hall mveh ,mip_organization mo   
    where  mveh.org_id = mo.id 
    <if test="orgList != null and currentOrg != null">
    and (mveh.org_id = #{currentOrg}
    </if>
    <if test="orgList != null" >
       	or( mveh.org_id in 
       	<foreach collection="orgList" item="item" open="(" close=")" separator="," >
       		#{item}
       	</foreach>
       	and mveh.status in('1', '3'))
   </if>
   <if test="orgList != null and currentOrg != null">
    )
    </if>
    <if test="key != null">
		and mveh.viName like '%${key}%' 
	</if>	
    <if test="type != null and type != ''">
		and mveh.viClassify = #{type} 
	</if>
	<if test="status != null and status != ''">
		and mveh.status = #{status} 
	</if>
	ORDER BY if(mveh.status='3',0,1), mveh.createTime desc
	<if test=" startRow == -1">
		limit #{pageSize}
	</if>
	<if test=" startRow != -1">
		limit #{startRow},#{pageSize}
	</if>
  </select>
  <select id="countVirtualList" resultType="java.lang.Integer">    
    select count(*)
    from mip_virtual_exibition_hall mveh 
    where 1=1
    <if test="orgList != null and currentOrg != null">
    and (mveh.org_id = #{currentOrg}
    </if>
    <if test="orgList != null" >
       	or( mveh.org_id in 
       	<foreach collection="orgList" item="item" open="(" close=")" separator="," >
       		#{item}
       	</foreach>
       	and mveh.status in('1', '3'))
   </if>
   <if test="orgList != null and currentOrg != null">
    )
    </if>
    <if test="key != null">
		and mveh.viName like '%${key}%' 
	</if>	
    <if test="type != null and type != ''">
		and mveh.viClassify = #{type} 
	</if>
	<if test="status != null and status != ''">
		and mveh.status = #{status} 
	</if>
  </select>
  
  <update id="publish">
    update mip_virtual_exibition_hall
    set status = #{status,jdbcType=VARCHAR},
    user_name  = #{userId,jdbcType=VARCHAR},
    publish_time  = #{publishTime,jdbcType=TIMESTAMP}
    where id = #{id,jdbcType=VARCHAR}
  </update>
</mapper>
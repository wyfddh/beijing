        <div class="item item-icon-left item-icon-right" ng-click="isLogin()">
            <i class="icon"><img src="img/146.png"></i>
            我的收藏
            <i class="icon ion-ios-arrow-forward"></i>
        </div>
        <div class="item item-icon-left item-icon-right" ng-click="myCezhan()">
            <i class="icon"><img src="img/pintu.png"></i>
            我的策展
            <i class="icon ion-ios-arrow-forward"></i>
        </div>
        <div class="item item-icon-left item-icon-right" ng-click="goChangePassWord()">
            <i class="icon"><img src="img/suo.png"></i>

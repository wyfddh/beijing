<div class="bar bar-header">
    <h1 id="shawn" class="title">
        <div id="login-title-box" class="login-title-box">
            <a href="#/login"><span>登录</span></a>/<a href="#/registerStepOne"><span>注册</span></a>
            <!--<a href="#/login"><span>登录</span></a>/<a><span>注册</span></a>-->
        </div>
    </h1>
    <h1 id="welcome" class="title"><div class="login-title-box"><span class="user_name">欢迎！{{userName}}</span><span ng-click="esc()" class="esc">注销</span></div></h1>

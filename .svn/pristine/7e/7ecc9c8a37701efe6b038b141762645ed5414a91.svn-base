<!--<h2 class="dili">中国 北京</h2>-->
<img class="person" src="img/first-person.png">
<section class="overflow-scroll has-subheader has-header scroll-content has-footer">
    <div class="list">
        <div class="item item-icon-left item-icon-right">
            <i class="icon"><img src="img/people.png"></i>
            个人信息
            <i class="icon ion-ios-arrow-forward"></i>
        </div>

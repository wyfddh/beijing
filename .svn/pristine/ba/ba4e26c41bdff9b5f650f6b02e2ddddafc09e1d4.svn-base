	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	font-size:16px;
	/**background-image: url(resources/fc/images/bj.jpg);*/
}
/**以下是对所有常见的对象的默认样式*/
INPUT,TEXTAREA,SELECT {
    FONT-SIZE: 16px;
	COLOR: #000000;
	FONT-FAMILY:"宋体", Arial, Helvetica, sans-serif;
	LINE-HEIGHT:24px;
	margin:0;
	padding-left:4;
	padding-right:4;
}
TH,TD{
    FONT-SIZE: 16px;
	/**COLOR: #000000; */
	FONT-FAMILY:"宋体", Arial, Helvetica, sans-serif;
	LINE-HEIGHT:24px;
	margin:0;
	padding-left:4;
	padding-right:4;
}

.ListHead {
	height:22px; 
	background-color:#E1F0F2;
	/**background:url(/images/main/list_bg1.jpg) repeat-x  top;*/
	font-size:16px;
	font-weight:normal;
	text-align:center;
	padding-left:4;
	padding-right:4;
	padding-top:1;
	padding-bottom:1;
	cursor:hand;
	/**
	background-color:#DCE3EB;
	border-bottom:4px #DCE3EB solid;
	border:1px inset;
	height:22px;
	border-left:1px  solid;
	border-top:0px;
	text-align:center;
	cursor:hand;
	*/
}

.ListDouble {
	height:22px; 
	background:#DCE3EB;
	font-size:16px;
	font-weight:normal;
	padding-left:4;
	padding-right:4;
	padding-top:1;
	padding-bottom:1;
}

.ListSingle {
	height:22px; 
	background:#fff;
	font-size:16px;
	font-weight:normal;
	padding-left:4;
	padding-right:4;
	padding-top:1;
	padding-bottom:1;
}

.read{
	height:21px;
	LINE-HEIGHT: 21px;
    cursor:hand;
	margin-top:0px;
	border-left:1px solid #D2D7D9; 
	border-top:1px solid #D2D7D9; 
	border-right:1px solid #D2D7D9; 
	border-bottom:1px solid #D2D7D9; 
	FONT-SIZE: 16px;
	padding:0;
	readonly:true;

}

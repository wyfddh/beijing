package com.tj720.admin.model.collection;

import java.io.Serializable;

/**
 * collection_info
 * @author 
 */
public class CollectionInfoWithBLOBs extends CollectionInfo implements Serializable {
    /**
     * 藏品简介
     */
    private String collectionIntroduction;

    /**
     * 备注
     */
    private String remark;

    private static final long serialVersionUID = 1L;

    public String getCollectionIntroduction() {
        return collectionIntroduction;
    }

    public void setCollectionIntroduction(String collectionIntroduction) {
        this.collectionIntroduction = collectionIntroduction;
    }

    public String getRemark() {
        return remark;
    }

    public void setRemark(String remark) {
        this.remark = remark;
    }
}
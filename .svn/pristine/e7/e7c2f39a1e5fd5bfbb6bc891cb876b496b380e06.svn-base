@charset "utf-8";

html {
	position: relative;
}

.rad {
	color: #FF0000;
}

.puls-o {
	width: 200px;
	height: 80px;
	border: 1px solid #FF0000;
}

.puls-o a {
	margin-left: 10px;
}

.but-rig {
	float: right;
}

.center {
	font-size: 16px;
}

.help-t {
	position: fixed;
	top: 100px;
	right: 70px;
	width: 400px;
	height: 450px;
	z-index: 23;
	border: 1px solid #3C537C;
	background-color: #FFFFFF;
}

.center tr {
	padding: 5px;
	height: 50px;
	border-right: 1px solid #DDDDDD;
	border-bottom: 1px solid #DDDDDD;
}

.center td {
	width: 190px;
}

.cb>input {
	height: 40px;
}

.center input {
	width: 100%;
}

.aa input {
	width: 60%;
}

.table-left {
	float: left;
}

.btu-right {
	width: 125px;
	float: right;
}

.btu-right>.btn {
	margin-top: 20px;
}

.but-bot>.btn {
	width: 100px;
	height: 40px;
	float: left;
}

.msu-cen img {
	width: 60px;
	padding: 3px;
}

.msu-cen th {
	height: 40px;
}

.msu-cen p {
	background-color: #EEEEEE;
	padding: 10px;
}

.add-nav input {
	width: 50px;
}

.add-nav {
	float: right;
	width: 300px;
}

.bolder-bot {
	border-bottom: 1px solid #CCCCCC;
}

.bolder-bot tr {
	height: 50px;
}
/*统计报表开始*/
#selectBox {
	width: 180px;
	background: #FFFFFF;
	position: fixed;
	top: 200px;
	left: 200px;
	z-index: 99;
}

.top-jibie {
	background: #ffffff;
	padding: 1px 10px;
	cursor: move;
}

.p-jibie {
	padding: 1px 10px;
}

.bt-jibie {
	border-top: 1px solid #ddd;
	padding: 5px;
	text-align: center;
}

#bgDiv {
	width: 100%;
	height: 100%;
	position: absolute;
	top: 0px;
	left: 0px;
	right: 0px;
	background-color: #777;
	opacity: 0.7;
}

.type-h3 {
	font-size: 22px;
}

.type-h3 {
	width: 300px;
	height: 30px;
	margin: 0 auto;
	font-size: 24px;
	text-align: center;
	margin-bottom: 10px;
}

.type-h3 span {
	border-bottom: 1px solid #CCCCCC;
}
/*统计报表结束*/
/*//指标管理*/
.rad {
	color: #FF0000;
}

.sec-nav {
	width: 100%;
	margin-top: 10px;
}

.span-right {
	padding-left: 30px;
}

.sec-nav .btn {
	margin-right: 10px;
}

.table-cen-le {
	width: 100%;
	border: 1px solid #CBCDCE;
	margin-top: 5px;
	text-align: center;
}

.ruten {
	text-align: right;
	margin-right: 40px;
}

.table-cen-le th {
	background: #eee;
}

.table-cen-le th, .table-cen-le td {
	height: 32px;
	text-align: center;
	border: 1px solid #CCCCCC;
}

.tree-ber {
	border: 1px solid #abbac3;
	min-height: 580px;
	border-radius: 10px;
}

.tree-ber-po {
	position: relative;
}

.tree-ber-shou:hover {
	background: #abbac3;
}

.tree-ber-shou {
	width: 10px;
	height: 100px;
	border: 1px solid #abbac3;
	background: #eee;
	position: absolute;
	top: 230px;
	right: 11px;
	border-bottom-left-radius: 10px;
	border-top-left-radius: 10px;
	line-height: 100px;
}

.tree-ber-shou img {
	width: 10px;
	height: 20px;
	line-height: 100px;
}

.sele-cen {
	margin-top: 10px;
}

.sele {
	border: 1px solid #abbac3;
	border-radius: 10px;
	min-height: 580px;
}

.tab-1 {
	width: 100%;
}

.tab-1 th {
	height: 50px;
	text-align: center;
	background-color: #5090c1;
}

.tab-1 td {
	border: 1px solid #5090c1;
	height: 40px;
	text-align: center;
}

.new-plus {
	width: 500px;
}

.new-plus-2 {
	width: 900px;
	height: 650px;
}

.new-plus, .new-plus-2 {
	background-color: #FFFFFF;
}

.new-top {
	height: 30px;
	background-color: #5090c1;
	color: #FFFFFF;
	font-size: 16px;
	padding: 5px;
	box-sizing: content-box;
}

.new-top:hover {
	opacity: 0.8;
}

.dele {
	color: #FFFFFF;
	line-height: 27px;
	text-align: center;
	float: right;
	margin-right: 10px;
}

.dele:hover {
	color: #FF0000;
}

.new-plus tr {
	height: 40px;
}

.new-plus th {
	text-align: right;
	padding: 5px;
	background-color: #F0F7FC;
}

.new-plus td {
	padding-left: 10px;
}

.new-plus select {
	width: 55%;
}

.new-plus .btn {
	margin-top: 10px;
	margin-left: 100px;
	width: 110px;
}

.table {
	font-size: 12px;
}

.table th {
	text-align: center;
}

.table tbody>tr>td {
	vertical-align: middle;
}

.new-an-top {
	height: 40px;
	padding: 5px;
}

.new-an-lf {
	border: 1px solid #5090c1;
	width: 250px;
	height: 500px;
	float: left;
}

.new-an-rg {
	width: 640px;
	float: right;
}

.new-an-rg, .new-an-lf {
	padding: 5px;
}

.rg-tab {
	height: 150px;
	border: 1px solid #5090c1;
	overflow: auto;
}

.rg-tab table {
	width: 100%;
	text-align: center;
}

.rg-tab table tr {
	height: 40px;
}

.rg-tab table tr>th {
	color: #FFFFFF;
	text-align: center;
	background-color: #5090c1;
}

.btu {
	padding: 10px;
	text-align: center;
}

.btu>button {
	height: 40px;
	width: 110px;
}

.col-xs-12 {
	padding-left: 0;
}

@media ( min-width : 900px) and (max-width:1000px) {
	body.navbar-fixed {
		padding-top: 80px;
	}
}

@media ( min-width : 500px) and (max-width:800px) {
	body.navbar-fixed {
		padding-top: 160px;
	}
}

@media ( max-width :480px) {
	body.navbar-fixed {
		padding-top: 240px;
	}
}
/*@media (min-width: 768px)
.tree-ber-po {
    width: 16.6666% !important;
}
.sele {
	width: 83.333% !important;
}*/
@media ( max-width :768px) {
	.tree-ber-po {
		width: 100% !important;
	}
	.sele {
		width: 100% !important;
	}
}
@charset "utf-8";
body{
	color: #555555;
}
.rad{
	color: #FF0000;
}
.sec-nav{
	float: left;
	width: 100%;	
    margin-top: 5px;   
	background-color: rgba(239, 243, 248,1);
}
.sec-nav .btn{
	margin-right: 10px;
	width: 120px;
}
.tab-1{		
	width: 100%;	
	color: #FFFFFF;
}
.tab-1 th{
	height: 50px;
	text-align: center;	
	background-color: #5090c1;
}
.tab-1 td{
	border: 1px solid  #5090c1;
	height: 40px;
	text-align: center;	
}
.new-plus{
  width: 500px;
  height: 550px;
  border: 1px solid #009CEB;
 background-color: #FFFFFF;
  position: fixed;
  margin:60px 0 0 50px;
}
.new-top{
	height: 30px;
	background-color: #5090c1;
	color: #FFFFFF;
	font-size: 16px;
	padding: 5px;
	box-sizing: content-box;
}
.dele{
	width: 30px;
	height: 30px;
	color: #FFFFFF;
	line-height: 27px;
	text-align: center;
	background-color: #FF0000;
	float: right;
}
.new-plus tr{	
	height: 40px;
}
.new-plus th{
	text-align: right;
	background-color:#F0F7FC;
}
.new-plus td{
	padding-left: 10px;
}
.new-plus select{
	width:55%;
}
.new-plus .btn{
	margin-top: 10px;
	margin-left: 100px;
	width: 110px;
}

@media ( max-width : 768px) {
	.grid-stack-item {
		position: relative !important;
		width: auto !important;
		left: 0 !important;
		top: auto !important;
		margin-bottom: 2px;
	}
	.grid-stack-item .ui-resizable-handle {
		display: none;
	}
	.grid-stack {
		height: auto !important;
	}
}


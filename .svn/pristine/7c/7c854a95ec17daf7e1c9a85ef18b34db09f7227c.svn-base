	box-shadow: 1px 4px 6px rgba(0, 0, 0, 0.2);
	opacity: 0.8;
}

.grid-stack>.grid-stack-item>.ui-resizable-se,.grid-stack>.grid-stack-item>.ui-resizable-sw
	{
	text-align: right;
	color: gray;
	padding: 2px 3px 0 0;
	margin: 0;

.platform-default #pf-hd {
	height: 70px;
	position: relative;
	z-index: 2;
	/**
	background-color: #208FDE;
	background: url('../images/main/xg_left.png?1464005190') left center
		no-repeat, url('../images/main/xg_right.png?1464005212') right center
		no-repeat, url('../images/main/top_bottombg.png?1464006836') left
		bottom repeat-x
		*/
}

.platform-default #pf-hd .pf-logo {
	float: left;
	padding: 0 15px;
	height: 100%;
	width: 300px;
	line-height: 70px
}

.platform-default #pf-hd .pf-logo img {
	vertical-align: middle;
	margin-left: -10px
}

.platform-default #pf-hd .pf-logo .system-name {
	display: none;
	float: left;
	line-height: 70px;
	font-size: 26px;
	color: #fff;
	text-shadow: 1px 1px 2px rgba(0, 0, 0, 0.5)
}

.platform-default #pf-hd .pf-nav {
	float: left
}

.platform-default #pf-hd .pf-nav .pf-nav-item {
	float: left;
	height: 70px;
	color: #fff;
	padding: 0 15px;
	border-right: 1px solid #5C6D7D
}

.platform-default #pf-hd .pf-nav .pf-nav-item a {
	display: block;
	color: #fff;
	text-decoration: none
}

.platform-default #pf-hd .pf-nav .pf-nav-item .iconfont {
	display: block;
	margin-top: 12px;
	font-size: 26px;
	height: 30px;
	text-align: center
}

.platform-default #pf-hd .pf-nav .pf-nav-item.current {
	/**background-color: #208FDE;*/
	background: url('../images/main/navbg.png?1465303472')
}

.platform-default #pf-hd .pf-nav .pf-nav-item.current a {
	color: #fff
}

.platform-default #pf-hd .pf-nav-wrap {
	position: relative;
	float: left;
	width: 90%;
	border-left: 1px solid #5C6D7D;
}

.platform-default #pf-hd .pf-nav-wrap .pf-nav-ww {
	float: left;
	height: 70px;
	width: 100%;
	overflow: hidden
}

.platform-default #pf-hd .pf-nav-prev,.platform-default #pf-hd .pf-nav-next {
	position: absolute;
	right: -32px;
	top: 10px;
	width: 26px;
	height: 23px;
	border-radius: 3px 3px 0 0;
	background: #208FDE;
	color: #fff;
	text-decoration: none;
	text-align: center;
	line-height: 23px
}

.platform-default #pf-hd .pf-nav-prev:hover,.platform-default #pf-hd .pf-nav-next:hover {
	background: #fff;
	color: #208FDE
}

#pf-hd .pf-nav-prev.disabled,.platform-default #pf-hd .pf-nav-next.disabled {
	color: #cccccc;
	background: #fff;
	cursor: default
}

.platform-default #pf-hd .pf-nav-next {
	top: 36px;
	border-radius: 0 0 3px 3px
}

.platform-default #pf-hd .pf-user {
	position: relative;
	float: right;
	padding: 0 10px;
	height: 70px;
	line-height: 70px;
	cursor: pointer;
	width: 150px;
}

.platform-default #pf-hd .pf-user .pf-user-photo {
	float: left;
	margin: 0px 10px 0 0;
	width: 48px;
	height: 48px
}

.platform-default #pf-hd .pf-user .pf-user-photo img {
	margin: 4px
}

.platform-default #pf-hd .pf-user .pf-user-name {
	float: left;
	max-width: 150px;
	color: #fff;
	font-size: 14px;
	margin-right: 5px
}

.platform-default #pf-hd .pf-user .xiala {
	color: #fff;
	position: relative;
	top: -2px
}

.platform-default #pf-hd .pf-user .pf-user-panel {
	display: none;
	position: absolute;
	left: 0;
	right: 0;
	top: 70px;
	background: #fff;
	z-index: 111;
	border: 1px solid #e1e1e1;
	box-shadow: 1px 1px 2px #ddd
}

.platform-default #pf-hd .pf-user .pf-user-panel li {
	height: 30px;
	line-height: 30px;
	font-size: 13px;
	padding: 0 15px
}

.platform-default #pf-hd .pf-user .pf-user-panel li a {
	color: #000;
	text-decoration: none
}

.platform-default #pf-hd .pf-user .pf-user-panel li .iconfont {
	margin-right: 10px;
	color: #000
}

.platform-default #pf-hd .pf-user .pf-user-panel li:hover {
	background: #f1f1f1
}

.platform-default #pf-hd .pf-user .pf-user-panel li:hover .iconfont {
	color: #208FDE
}

.platform-default #pf-hd .pf-user:hover {
	background: url('../images/main/top_userbg.png?1464007598')
}

.platform-default #pf-hd .pf-user:hover .pf-user-panel {
	display: block
}

.platform-default #pf-bd {
	position: absolute;
	top: 29px;
	bottom: 31px;
	left: 0;
	right: 0;
	padding: 0 0 0 220px;
	z-index: 1;
	transition: all 0.3s ease
}

.platform-default #pf-bd.toggle {
	padding-left: 0
}

.platform-default #pf-bd.toggle #pf-sider {
	margin-left: -440px
}

.platform-default #pf-bd.toggle #pf-sider .pf-model-name .toggle-icon {
	position: fixed;
	top: 84px;
	left: 0;
	z-index: 10;
	background-position: -22px 0
}

.platform-default #pf-bd #pf-sider {
	float: left;
	height: 100%;
	margin-left: -220px;
	width: 223px;
	background: #fff;
	overflow-y: auto;
	overflow-x: hidden;
	z-index: 1;
	transition: all 0.3s ease
}

.platform-default #pf-bd #pf-sider .pf-model-name {
	position: relative;
	padding: 0 20px;
	height: 49px;
	line-height: 49px;
	color: #2d4112;
	background: #e3e3e3;
	border-bottom: 1px solid #bdbdbd;
	font-size: 16px
}

.platform-default #pf-bd #pf-sider .pf-model-name .iconfont {
	font-size: 20px
}

.platform-default #pf-bd #pf-sider .pf-model-name .pf-sider-icon {
	float: left;
	margin: 10px 15px 0 0;
	width: 20px;
	height: 20px;
	font-size: 20px
}

.platform-default #pf-bd #pf-sider .pf-model-name .toggle-icon {
	position: absolute;
	right: 0;
	top: 14px;
	width: 19px;
	height: 22px;
	background: url(../images/main/sider-toggle.png) no-repeat;
	background-position: 0 0;
	cursor: pointer
}

.platform-default #pf-bd #pf-sider .sider-nav>li {
	border-bottom: 1px solid #dbdbdb;
	background: #fff
}

.platform-default #pf-bd #pf-sider .sider-nav>li a {
	display: block;
	padding: 0 15px;
	height: 100%;
	height: 50px;
	line-height: 50px;
	color: #000;
	font-size: 14px;
	text-decoration: none
}

.platform-default #pf-bd #pf-sider .sider-nav>li .sider-nav-title {
	float: left;
	width: 112px;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
	font-size: 14px
}

.platform-default #pf-bd #pf-sider .sider-nav>li .iconfont {
	float: right;
	font-size: 12px;
	color: #acacab
}

.platform-default #pf-bd #pf-sider .sider-nav>li .sider-nav-icon {
	float: left;
	margin: 0 10px 0 0;
	color: #293846;
	font-size: 20px
}

.platform-default #pf-bd #pf-sider .sider-nav>li.current {
	border-bottom: none;
	background: #293846
}

.platform-default #pf-bd #pf-sider .sider-nav>li.current>a {
	border-left: 3px solid #ffa405;
	padding-left: 12px;
	color: #fff
}

.platform-default #pf-bd #pf-sider .sider-nav>li.current .iconfont {
	color: #fff
}

.platform-default #pf-bd #pf-sider .sider-nav>li.current .sider-nav-icon {
	color: #fff
}

.platform-default #pf-bd #pf-sider .sider-nav>li .sider-nav-s {
	display: none;
	padding: 10px 0;
	background: #f1f1f1;
	color: #000;
	font-size: 12px;
	border-bottom: 1px solid #dbdbdb
}

.platform-default #pf-bd #pf-sider .sider-nav>li .sider-nav-s li.active {
	background: #fff
}

.platform-default #pf-bd #pf-sider .sider-nav>li .sider-nav-s li.active a {
	color: #208FDE;
	font-weight: bold
}

.platform-default #pf-bd #pf-sider .sider-nav>li .sider-nav-s a {
	height: 31px;
	line-height: 31px;
	color: #000;
	font-size: 12px;
	padding-left: 40px;
	background: url('../images/main/left_list.png?1463555463') no-repeat
		30px center
}

.platform-default #pf-bd #pf-sider .sider-nav>li.current .sider-nav-s {
	display: block
}

